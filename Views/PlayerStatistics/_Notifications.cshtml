<script src="~/lib/jquery/dist/jquery.min.js"></script>
<link href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css" rel="stylesheet" />
<script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js"></script>
@if (TempData["successPlayerStatistics"] != null)
{
    <script type="text/javascript">
        toastr.success('@TempData["successPlayerStatistics"]');
    </script>
}

@if (TempData["warningPlayerStatistics"] != null)
{
    <script type="text/javascript">
        toastr.warning('@TempData["warningPlayerStatistics"]');
    </script>
}

@if (TempData["errorPlayerStatistics"] != null)
{
    <script type="text/javascript">
        toastr.error('@TempData["errorPlayerStatistics"]');
    </script>
}
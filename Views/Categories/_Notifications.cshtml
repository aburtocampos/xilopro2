<script src="~/lib/jquery/dist/jquery.min.js"></script>
<link href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css" rel="stylesheet" />
<script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js"></script>
@if (TempData["successCategory"] != null)
{
    <script type="text/javascript">
           toastr.success('@TempData["successCategory"]');
    </script>
}

@if (TempData["warningCategory"] != null)
{
    <script type="text/javascript">
        toastr.warning('@TempData["warningCategory"]');
    </script>
}

@if (TempData["errorCategory"] != null)
{
    <script type="text/javascript">
           toastr.error('@TempData["errorCategory"]');
    </script>
}
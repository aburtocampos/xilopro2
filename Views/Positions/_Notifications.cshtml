<script src="~/lib/jquery/dist/jquery.min.js"></script>
<link href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css" rel="stylesheet" />
<script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js"></script>
@if (TempData["successPosition"] != null)
{
    <script type="text/javascript">
           toastr.success('@TempData["successPosition"]');
    </script>
}

@if (TempData["warningPosition"] != null)
{
    <script type="text/javascript">
        toastr.warning('@TempData["warningPosition"]');
    </script>
}

@if (TempData["errorPosition"] != null)
{
    <script type="text/javascript">
           toastr.error('@TempData["errorPosition"]');
    </script>
}
@model xilopro2.Data.Entities.Country

@{
    ViewData["Title"] = "Create";
}


<div class="container">
    <h2><a asp-action="Index" class="btn btn-outline-default"><i class="fa-solid fa-arrow-left"></i> Atras</a> | Creando Nuevo País</h2>
    <hr />

    <form class="card border-primary" style="width: 60%;margin:0 auto;" asp-action="Create">
        <div class="card-body">
            <div class="col-md-12">

                <partial name="_FormCountry" />

            </div>
        </div>
        <div class="card-footer card-footer-primary">
            <div class="form-group my-2 d-flex justify-content-between">
                <a asp-action="Index" class="btn btn-outline-default"><i class="fa-solid fa-ban"></i> Cancelar</a>
                <button type="submit" value="Save" class="btn btn-primary"><i class="fa-regular fa-floppy-disk"></i> Guardar</button>
            </div>
        </div>
    </form>

</div>


@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}

<script src="~/lib/jquery/dist/jquery.min.js"></script>
<link href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css" rel="stylesheet" />
<script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js"></script>
@if (TempData["successCountry"] != null)
{
    <script type="text/javascript">
           toastr.success('@TempData["successCountry"]');
    </script>
}

@if (TempData["warningCountry"] != null)
{
    <script type="text/javascript">
        toastr.warning('@TempData["warningCountry"]');
    </script>
}

@if (TempData["errorCountry"] != null)
{
    <script type="text/javascript">
           toastr.error('@TempData["errorCountry"]');
    </script>
}




